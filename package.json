{
  "name": "@nx-devkit/cli",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "bin": "rm -rf node_modules/.bin && npm i",
    "preversion": "nx affected --target=version --base=v${npm_old_version} ${npm_new_version} --workspaces-update=false",
    "version": "npm install --silent --ignore-scripts --package-lock-only --fund=false --audit=false && git add -A",
    "nx:publish": "nx run-many --target=publish --projects=$(get-projects-to-publish)"
  },
  "private": true,
  "dependencies": {
    "execa": "^7.1.1"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "16.5.4",
    "@nx/js": "16.5.4",
    "@nx/linter": "16.5.4",
    "@nx/workspace": "16.5.4",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "nx": "16.5.4",
    "nx-cloud": "latest",
    "prettier": "^2.6.2",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
